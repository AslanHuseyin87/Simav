@model IEnumerable<Simav.Models.Personel>

@{
    ViewData["Title"] = "BaskanYardimcilari";
}

<div role="main" class="main">

				<section class="page-top">
					<div class="container">
						<div class="row">
							<div class="col-md-12">
								<ul class="breadcrumb">
									<li><a href="default.asp">Ana Sayfa</a></li>
									<li class="active">Kurumsal</li>
									<li class="active">Başkan Yardımcıları</li>
								</ul>
							</div>
						</div>
						<div class="row">
							<div class="col-md-12">
								<h1>Başkan Yardımcıları</h1>
							</div>
						</div>
					</div>
				</section>

				<div class="container">


					


					<div class="row">

						<ul class="team-list sort-destination" data-sort-id="team">
					 @foreach(var item in Model){	
							<li class="col-md-3 col-sm-6 col-xs-12 isotope-item leadership">
								<div class="team-item thumbnail">
									<a asp-asp-controller="Personel" asp-action="PersonelCV" asp-route-id="@item.PersonelId" class="thumb-info team">
										<img class="img-responsive" alt="" src="~/images/Personel_Image/@item.Resim">
										<span class="thumb-info-title">
											<span class="thumb-info-inner">@item.AdSoyad</span>
											<span class="thumb-info-type">@item.Unvan</span>
										</span>
									</a>
								</div>
							</li>
					}
						</ul>

					</div>

				</div>

			</div>
